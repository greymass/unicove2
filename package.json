		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/big.js": "^6.2.2",
		"@types/eslint": "^8.56.11",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.7.0",
